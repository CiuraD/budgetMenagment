              "*/*": {
                "schema": {
                  "$ref": "#/components/schemas/RoomUserDto"
                }
              }
            }
          }
        }
      }
    },
    "/rooms/{roomId}/products": {
      "get": {
        "tags": [
          "room-controller"
        ],
        "summary": "List products in a room",
        "operationId": "listProducts",
        "parameters": [
          {
            "name": "roomId",
