  "components": {
    "schemas": {
      "RoomDto": {
        "type": "object",
        "properties": {
          "roomId": {
            "type": "integer",
            "format": "int64"
          },
          "roomName": {
            "type": "string",
            "minLength": 1
          }
        },
        "required": [
          "roomName"
        ]
      },
      "RoomUserDto": {
        "type": "object",
        "properties": {
          "userId": {
            "type": "integer",
            "format": "int64"
          },
          "roomId": {
            "type": "integer",
            "format": "int64"
          },
          "admin": {
            "type": "boolean"
          }
        },
        "required": [
          "userId"
        ]
      },
      "RoomProductDto": {
        "type": "object",
        "properties": {
          "productId": {
            "type": "integer",
            "format": "int64"
          },
          "roomId": {
            "type": "integer",
            "format": "int64"
          },
          "productName": {
            "type": "string",
            "minLength": 1
          },
          "price": {
            "type": "number"
          },
          "paid": {
            "type": "boolean"
          },
          "isPaid": {
            "type": "boolean"
          }
        },
        "required": [
          "price",
          "productName"
        ]
      }
    },
    "securitySchemes": {
      "bearer-jwt": {
        "type": "http",
        "name": "Authorization",
        "scheme": "bearer",
        "bearerFormat": "JWT"
      }
    }
  }
